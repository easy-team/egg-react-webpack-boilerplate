  "description": "基于 easywebpack-react 和 egg-view-react-ssr 插件服务端渲染工程骨架项目",
  "scripts": {
    "build": "cross-env easy build prod",
    "build:test": "cross-env easy build test",
    "build:prod": "cross-env easy build prod",
    "dev": "cross-env WORKERS=1 node index.js",
    "start": "cross-env EGG_SERVER_ENV=prod node index.js",
    "start:test": "cross-env EGG_SERVER_ENV=test node index.js",
    "kill-port": "kill -9 $(lsof -i:8888 |grep 'node'|aw '{print $2}') ",
