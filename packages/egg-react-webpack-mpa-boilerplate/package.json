{
  "name": "egg-react-webpack-mpa-boilerplate",
  "version": "5.1.0-alpha.0",
  "description": "Egg React Server Side Render Examples",
  "scripts": {
    "clean": "easy clean all",
    "build": "easy build",
    "debug": "egg-bin debug",
    "dev": "egg-bin dev",
    "test": "egg-bin test",
    "start": "egg-scripts start --port 7001 --workers 4",
    "backend": "nohup egg-scripts start --port 7001 --workers 4 &",
    "lint": "eslint . --ext .js,jsx",
    "fix": "eslint --fix . --ext .js,jsx",
    "ii": "yarn --registry https://registry.npmmirror.com",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js, jsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "antd": "^4",
    "axios": "^0.21.1",
    "egg": "^2.1.0",
    "egg-cors": "^2.0.0",
    "egg-logger": "^2.4.1",
    "egg-scripts": "^2.8.1",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.0.0",
    "egg-view-react-ssr": "^3.0.0",
    "extend": "~3.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mockjs": "^1.1.0",
    "moment": "^2.17.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.5",
    "easywebpack-cli": "^5.0.0",
    "easywebpack-react": "^5.0.0",
    "egg-bin": "^4.5.0",
    "egg-mock": "^4.0.1",
    "egg-webpack": "^5.0.0",
    "egg-webpack-react": "^3.0.0",
    "eslint": "^7.2.0",
    "eslint-config-egg": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "husky": "^4.2.5",
    "ip": "^1.1.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.2",
    "raw-loader": "^4.0.0",
    "service-worker-register": "^1.2.6"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10, 12, 14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easy-team/egg-react-webpack-boilerplate.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/easy-team/egg-react-webpack-boilerplate"
}
